{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main", "prefs"],
  "permissions": [
    "core:default",
    "core:window:allow-show",
    "core:window:allow-center",
    "core:window:allow-create",
    "core:window:allow-set-focus",
    "core:window:allow-set-title",
    "core:window:allow-set-theme",
    "core:webview:allow-create-webview-window",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": ["**"]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "shell:default",
    "fs:default",
    "dialog:default",
    "store:default"
  ]
}
